{
    "configurations": [
        {
            "name": "Pico FreeRTOS",
            "includePath": [
                "${workspaceFolder}/**",
                "${userHome}/.pico-sdk/sdk/2.1.1/**",
                "${workspaceFolder}/include",
                "${env:FREERTOS_KERNEL_PATH}/include",
                "${env:FREERTOS_KERNEL_PATH}/portable/ThirdParty/GCC/RP2040/include",
                "${workspaceFolder}/lib/Ezo_I2c_lib_pico/include",
                "${userHome}/.pico-sdk/sdk/2.1.1/src/rp2_common/hardware_sync/include",
                "${userHome}/.pico-sdk/sdk/2.1.1/src/rp2_common/hardware_sio/include",
                "${userHome}/.pico-sdk/sdk/2.1.1/src/rp2_common/hardware_i2c/include"
            ],
            "forcedInclude": [
                "${userHome}/.pico-sdk/sdk/2.1.1/src/common/pico_base_headers/include/pico.h",
                "${workspaceFolder}/build/generated/pico_base/pico/config_autogen.h"
            ],
            "defines": [
                "PICO_BOARD=\"rp2040_PURPLE_USBC_16M_NONRGB\"",
                "PICO_32BIT=1",
                "PICO_RP2040=1",
                "projCOVERAGE_TEST=0"
            ],
            "compilerPath": "${userHome}/.pico-sdk/toolchain/14_2_Rel1/bin/arm-none-eabi-gcc",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}